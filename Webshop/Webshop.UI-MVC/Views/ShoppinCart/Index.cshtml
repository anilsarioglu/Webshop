@using Webshop.UI_MVC.Models.Webshop
@model IEnumerable<Webshop.UI_MVC.Models.Webshop.Course>

@{
    ViewBag.Title = "Index";
}
<h3>Cart Page</h3>
<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Price</th>
        <th>Quantity</th>
    </tr>
</table>
@foreach (Course item in (List<Course>)Session["cart"])
{
    <tr>
        <td>@item.Product.Id</td>
        <td>@item.Product.Name</td>
        <td>@item.InvoiceDetail.Pieces</td>
    </tr>
}
