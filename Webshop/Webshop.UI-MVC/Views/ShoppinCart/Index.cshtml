@using Webshop.UI_MVC.Models.Webshop
@model IEnumerable<Webshop.UI_MVC.Models.Webshop.ShoppingCard>

@{
    ViewBag.Title = "Index";
}
<h3>Cart Page</h3>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course.Name)
        </th>
        <th></th>
    </tr>
</table>
@foreach (ShoppingCard item in (List<ShoppingCard>)Session["cart"])
{
    <td>
        @Html.DisplayFor(modelItem => item.Course.Name)
    </td>
}
